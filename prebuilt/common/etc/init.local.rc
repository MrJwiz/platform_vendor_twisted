# Extras


#    start sysinit

on property:sys.boot_completed=1
    start userinit

# sysinit (/system/etc/init.d)
service sysinit /system/bin/sysinit
    user root
    group root
    oneshot

# runparts (/system/etc/init.d)
service userinit /system/xbin/busybox run-parts /system/etc/init.d
	oneshot
	class late_start
	user root
	group root
